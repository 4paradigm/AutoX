   "metadata": {},
   "outputs": [],
   "source": [
    "# identify path; subject to change\n",
    "#path_train = 'Your_directory/MINDsmall_train/'\n",
    "#path_valid = 'Your_directory/MINDsmall_dev/'\n",
    "path_train = 'Your_directory/MINDlarge_train/'\n",
    "path_valid = 'Your_directory/MINDlarge_dev/'\n",
    "\n",
    "# read data\n",
    "# training:\n",
    "train_behave_path = path_train + \"behaviors.tsv\"\n",
    "train_news_path = path_train + \"news.tsv\"\n",
    "list_col_behave = ['Impression_id', 'user_id', 'time', 'hist', 'impressions']\n",
    "list_col_news = ['News_ID','Category','SubCategory','Title','Abstract','URL','Title_enti','Abstract_enti']\n",
    "\n",
    "# testing data:\n",
    "train_behave_path_valid = path_valid + \"behaviors.tsv\"\n",
    "train_news_path_valid = path_valid + \"news.tsv\"\n",
    "\n",
    "\n",
    "#print(train_behave_path)\n",
    "df_behave = pd.read_csv(train_behave_path, sep=\"\\t\", header=None, names=list_col_behave)\n",
    "df_news = pd.read_csv(train_news_path, sep=\"\\t\", header=None, names=list_col_news)\n",
    "df_behave_valid = pd.read_csv(train_behave_path_valid, sep=\"\\t\", header=None, names=list_col_behave)\n",
    "df_news_valid = pd.read_csv(train_news_path_valid, sep=\"\\t\", header=None, names=list_col_news)\n",
    "\n",
    "df_behave['time'] = pd.to_datetime(df_behave['time'], format='%m/%d/%Y %I:%M:%S %p')"
   ]
  },
