    "# 备用"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8614ac9d",
   "metadata": {},
   "source": [
    "embedding data loading (training)\n",
    "path_emd_entity = 'Your_directory/entity_embedding.vec'\n",
    "path_emd_relation = 'Your_directory/relation_embedding.vec'\n",
    "\n",
    "\n",
    "def search_Qid(row_dummy):\n",
    "    return re.search(r'Q\\d*', row_dummy).group()\n",
    "\n",
    "def split_vec_vals(row_dummy):\n",
    "    temp = re.split('\\t', row_dummy)\n",
    "    del (temp[0])\n",
    "    del (temp[-1])\n",
    "    temp = list(map(convert_list_str_float, temp))\n",
    "    return temp\n",
    "\n",
    "def convert_list_str_float(dum):\n",
    "    try:\n",
    "        cov_dum = float(dum)\n",
    "    except:\n",
    "        cov_dum = np.nan\n",
    "    \n",
    "    return cov_dum\n",
    "\n",
    "list_col_name = []\n",
    "for i in range(100):\n",
    "    list_col_name.append('vec'+str(i))\n",
    "\n",
    "def DataFrame_vecs(path_emd):\n",
    "    file_emd = open(path_emd)\n",
    "    lines_temp = file_emd.readlines()\n",
    "    # entity embeddings\n",
    "    S_entity_emd = pd.Series(lines_temp)\n",
    "    col_Qid = S_entity_emd.apply(search_Qid)\n",
    "    cols_vecs = S_entity_emd.apply(split_vec_vals)\n",
    "    df_vecs = pd.DataFrame(list(cols_vecs), columns=list_col_name)\n",
    "    df_emd_vecs = pd.concat([col_Qid, df_vecs], axis=1)\n",
    "    df_emd_vecs.rename({0:'Entity_id'}, axis=1, inplace=True)\n",
    "    file_emd.close()\n",
    "    \n",
    "    return df_emd_vecs\n",
    "\n",
    "df_emd_vecs_entity = DataFrame_vecs(path_emd_entity)\n",
    "df_emd_vecs_relation = DataFrame_vecs(path_emd_relation)\n",
    "\n",
    "\n",
    "        "
   ]
  },
