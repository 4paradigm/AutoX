   "source": [
    "df_behave_sort = df_behave.sort_values(['user_id','time'])\n",
    "#print(df_behave_sort)\n",
    "\n",
    "def find_pos_clicks(row_dummy):\n",
    "    temp = re.findall('N[0-9]*-1', row_dummy)\n",
    "    temp = ' '.join(temp)\n",
    "    if len(temp) != 0:\n",
    "        temp = re.findall('N[0-9]*', temp)\n",
    "        return temp\n",
    "\n",
    "    else:\n",
    "        return None\n",
    "    #print(len(temp))\n",
    "    #temp = ' '.join(temp)\n",
    "\n",
    "df_behave_sort['impression_positive_clicks'] = df_behave_sort['impressions'].apply(find_pos_clicks)\n",
    "#print(df_behave_sort.loc[106163, 'impression_clicks'])\n",
    "#print(df_behave_sort)\n",
